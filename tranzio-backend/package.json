{
  "name": "tranzio-backend",
  "version": "1.0.0",
  "description": "Tranzio Escrow Trading Platform Backend API",
  "main": "dist/main-server.js",
  "scripts": {
    "dev": "nodemon main-server.ts",
    "build": "tsc --noEmit false && prisma generate",
    "start": "node dist/main-server.js",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "escrow",
    "trading",
    "platform",
    "api",
    "nodejs",
    "typescript"
  ],
  "author": "Tranzio Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^7.0.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.7.0",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  }
}
